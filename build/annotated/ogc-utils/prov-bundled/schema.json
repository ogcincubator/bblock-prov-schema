          "x-jsonld-type": "@id"
        },
        "qualifiedInfluence": {
          "oneOf": [
            {
              "$ref": "#/$defs/objectref"
            },
            {
              "$ref": "#/$defs/Influence"
            },
            {
              "type": "array",
              "items": {
                "oneOf": [
                  {
                    "$ref": "#/$defs/objectref"
                  },
                  {
                    "$ref": "#/$defs/Influence"
