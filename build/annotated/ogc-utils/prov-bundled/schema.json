        {
          "required": [
            "entityType"
          ]
        },
        {
          "required": [
            "wasGeneratedBy"
          ]
        },
        {
          "required": [
            "wasAttributedTo"
          ]
        },
        {
          "required": [
            "wasDerivedFrom"
          ]
        },
        {
          "required": [
            "has_provenance"
          ]
        },
        {
          "properties": {
            "type": {
              "type": "string",
              "const": "Collection"
            },
            "hadMember": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/Entity"
              },
              "x-jsonld-id": "http://www.w3.org/ns/prov#hadMember",
              "x-jsonld-type": "@id"
            }
          },
          "required": [
            "hadMember",
            "type"
          ]
        },
        {
          "properties": {
            "type": {
              "type": "string",
              "const": "EmptyCollection"
            },
            "hadMember": {
              "type": "array",
              "maxItems": 0,
              "x-jsonld-id": "http://www.w3.org/ns/prov#hadMember",
              "x-jsonld-type": "@id"
            }
          },
          "required": [
            "hadMember",
            "type"
          ]
        }
      ],
      "allOf": [
        {
          "$ref": "#/$defs/influenced"
        }
      ]
