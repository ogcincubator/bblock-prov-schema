        {
          "$ref": "#/$defs/Agent"
        },
        {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/objectref"
              },
              {
                "$ref": "#/$defs/Agent"
              }
            ]
          }
        }
      ]
    },
    "externalLink": {
      "$ref": "https://opengeospatial.github.io/bblocks/annotated-schemas/ogc-utils/json-link/schema.json"
    },
