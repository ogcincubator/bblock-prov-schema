          "x-jsonld-id": "@type"
        },
        "entityType": {
          "$ref": "#/$defs/oneOrMoreObjectref",
          "x-jsonld-id": "@type"
        },
        "has_provenance": {
          "$ref": "#/$defs/Prov",
