          "required": [
            "prov:type"
          ]
        },
        {
          "properties": {
            "type": {
              "$ref": "#/$defs/EntityTypes"
            }
          },
          "required": [
            "type"
          ]
        },
        {
          "required": [
            "featureType"
          ]
